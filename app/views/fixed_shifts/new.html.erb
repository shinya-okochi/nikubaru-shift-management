<%= form_for(@fixed_shift) do |f| %>
  <%= f.label :user_id, "ユーザー：" %>
  <%= f.number_field :user_id %>
  <%= f.label :date, "日程：" %>
  <%= f.date_field :date %>
  <%= f.label :start_time, "開始時間：" %>
  <%= f.time_select :start_time %>
  <%= f.label :end_time, "終了時刻：" %>
  <%= f.time_select :end_time %>
  <%= f.submit "送信" %>
<% end %>